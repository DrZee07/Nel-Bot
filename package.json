{
  "name": "vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "tsx watch backend/server.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "vite build",
    "build:backend": "tsc backend/server.ts --outDir dist/backend",
    "start:backend": "node dist/backend/server.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "security:audit": "npm audit --audit-level moderate",
    "security:check": "node backend/scripts/security-check.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:medical": "vitest run --reporter=verbose src/lib/__tests__/",
    "test:components": "vitest run --reporter=verbose src/components/__tests__/",
    "test:backend": "vitest run --reporter=verbose backend/**/*.test.{ts,js}",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:medical": "playwright test tests/e2e/medical-scenarios.spec.ts",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps"
  },
